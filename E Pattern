function printE(n) {
    for (let i = 0; i < n; i++) {
        let row = "";

        // Top, middle, and bottom horizontal lines
        if (i === 0 || i === Math.floor(n / 2) || i === n - 1) {
            for (let j = 0; j < n; j++) row += "* ";
        }
        // Vertical line on the left
        else {
            row += "*";
        }

        console.log(row);
    }
}

// Call the function
printE(7);
