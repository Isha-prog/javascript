function printI(n) {
    for (let i = 0; i < n; i++) {
        let row = "";

        // Top and Bottom horizontal line
        if (i === 0 || i === n - 1) {
            for (let j = 0; j < n; j++) row += "* ";
        } 
        else {
            // Middle vertical line
            for (let j = 0; j < Math.floor(n / 2); j++) row += "  "; 
            row += "*";
        }

        console.log(row);
    }
}

// Call the function
printI(5);
