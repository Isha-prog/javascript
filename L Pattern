function printL(n) {
    for (let i = 0; i < n; i++) {
        let row = "";

        if (i < n - 1) {
            // Vertical Line
            row += "*";
        } else {
            // Last row - full horizontal line
            for (let j = 0; j < n; j++) {
                row += "* ";
            }
        }
        console.log(row);
    }
}

printL(5);
