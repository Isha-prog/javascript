function printF(n) {
    for (let i = 0; i < n; i++) {
        let row = "";

        // Top horizontal line
        if (i === 0) {
            for (let j = 0; j < n; j++) row += "* ";
        }
        // Middle horizontal line
        else if (i === Math.floor(n / 2)) {
            for (let j = 0; j < n - 1; j++) row += "* ";
        }
        // Vertical line
        else {
            row += "*";
        }

        console.log(row);
    }
}

// Call the function
printF(7);
